public class LoopPattern {
    public static void main(String[] args) {
        int a = 6, b = 2;
        for (int i = 0; i < 3; i++) {
            System.out.print(a + " " + b + " ");
            a++;
            b--;
        }
        System.out.print(a + " " + b + " " + (a + 1)); // prints the rest (9 -1 10)
    }
}
